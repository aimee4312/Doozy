{
  "expo": {
    "name": "doozy",
    "slug": "doozy",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "assetBundlePatterns": [
      "**/*"
    ],
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.doozy"
    },
    "android": {
      "package": "com.anonymous.doozy"
    },
    "plugins": [
        [
        "expo-build-properties",
          {
          "ios": {
            "useFrameworks": "static",
            "modularHeaders": {
              "FirebaseCore": true,
              "FirebaseCoreInternal": true,
              "GoogleUtilities": true,
              "FirebaseAuth": true,
              "FirebaseStorage": true,
              "FirebaseFirestore": true,
              "FirebaseCrashlytics": true
            }
          }
        }
      ]
    ]
  }
}
