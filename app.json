{
  "expo": {
    "name": "Doozy",
    "slug": "Doozy",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "icon": "./src/assets/doozy_logo2_nt.png",
    "assetBundlePatterns": [
      "**/*"
    ],
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.doozy"
    },
    "android": {
      "package": "com.anonymous.doozy"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static",
            "modularHeaders": {
              "FirebaseCore": true,
              "FirebaseCoreInternal": true,
              "GoogleUtilities": true,
              "FirebaseAuth": true,
              "FirebaseStorage": true,
              "FirebaseFirestore": true,
              "FirebaseCrashlytics": true
            }
          }
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends.",
          "cameraPermission": "The app uses your camera to let you take photos."
        }
      ],
      "expo-font"
    ]
  }
}
